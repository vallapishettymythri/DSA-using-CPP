#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int n; 
    cin >> n;
    int multiply=1;
    for(int i=1;i<=10;i++){
        multiply=n*i;
        cout << n << " " << "*" <<" " << i << " " << '=' << " " << multiply << endl;
    }
    cout << endl;
    return 0;
}
