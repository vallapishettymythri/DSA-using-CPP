
int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int n;
    cin >> n;
    int a[n];
    for(int i=1;i<=n;i++){
        cin >> a[i];
    }
    int even=0;
    int odd=0;
    for(int i=1;i<=n;i++){
        if(a[i]%2==0){
            even++;
        }else{
            odd++;
        }
    }
    cout << even << " " << odd;
    return 0;
}
