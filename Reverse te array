#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int n;
    cin >> n;
    int a[n];
    for(int i=0;i<=n;i++){
        cin >> a[i];
    }
    int temp;
    for(int i=0,j=n-1;i<j;i++,j--){
        temp=a[i];
        a[i]=a[j];
        a[j]=temp;
    }
    for(int i=0;i<n;i++){
        cout << a[i] <<" ";
    }
    return 0;
}
