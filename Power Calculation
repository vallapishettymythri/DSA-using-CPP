#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    int a,b;
    cin >> a >> b;
    int powers=1;
    for(int i=1; i<=b;i++){
         powers*=a;
    }
    cout << powers;
    return 0;
}
