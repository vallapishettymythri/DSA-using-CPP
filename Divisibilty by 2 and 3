#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int a;
    cin >> a;
    if(a%2==0 & a%3==0){
        cout << "Yes";
    }
    else{
        cout << "No";
    }
    return 0;
}
